Sub Header()
    Dim filePath As String, fso As Object, ts As Object
    Dim ws As Worksheet, line As String, lineCount As Long
    Dim buffer(1 To 2) As String, parts As Variant
    Dim i As Long, r As Long, c As Long
    
    ' --- Select .GID file ---
    filePath = Application.GetOpenFilename("Text Files (*.GID),*.GID", , "Select the .GID file to read")
    If filePath = "False" Then Exit Sub
    
    ' --- Open the file for reading ---
    Set fso = CreateObject("Scripting.FileSystemObject")
    Set ts = fso.OpenTextFile(filePath, 1)
    Set ws = ActiveSheet
    
    ' --- Read lines and store 19â€“22 ---
    lineCount = 0
    Do Until ts.AtEndOfStream
        lineCount = lineCount + 1
        line = ts.ReadLine
        
        ' Store target lines
        If lineCount = 19 Then buffer(1) = line
        If lineCount = 20 Then buffer(1) = buffer(1) & line
        If lineCount = 21 Then buffer(2) = line
        If lineCount = 22 Then buffer(2) = buffer(2) & line
    Loop
    ts.Close
    
    ' --- Process two combined lines ---
    For r = 1 To 2
        If buffer(r) <> "" Then
            ' Split by single quote
            parts = Split(buffer(r), "'")
            c = 1
            ' Extract values between quotes and write to cells
            For i = 1 To UBound(parts) Step 2
                ws.Cells(r, c).Value = parts(i)
                c = c + 1
            Next i
        End If
    Next r
    
    MsgBox "Header extraction completed.", vbInformation
End Sub
