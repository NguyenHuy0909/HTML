Sub SplitByQuotes()
    Dim txt As String
    Dim parts() As String
    Dim ws As Worksheet
    Dim i As Long, count As Long
    
    ' Gán chuỗi nguồn
    txt = "CHANNEL = ['AngR','Time','Disp','Velo','Acce','Load','Forc','Efrc','BLod']"
    Set ws = ActiveSheet
    
    ' Tách chuỗi bằng dấu nháy đơn (')
    parts = Split(txt, "'")
    
    ' Mỗi cặp nháy đơn tạo ra 1 phần tử cần lấy
    ' Ký tự nằm ở vị trí lẻ: 1, 3, 5, 7, ...
    count = 1
    For i = 1 To UBound(parts) Step 2
        ws.Cells(1, count).Value = parts(i)
        count = count + 1
    Next i
End Sub
