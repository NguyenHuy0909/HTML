Public Function GetColorArray(ByVal rng As Range) As Variant

    Dim arr() As Long
    Dim i As Long
    
    ReDim arr(1 To rng.Rows.Count)
    
    For i = 1 To rng.Rows.Count
        arr(i) = rng.Cells(i, 1).Interior.Color
    Next i
    
    GetColorArray = arr

End Function

colorArr = GetColorArray(Sheets("Config").Range("D27:D36"))
