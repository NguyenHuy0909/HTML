Sub ThemSeriesMoi()
    Dim cht As ChartObject
    Dim newSeries As Series

    ' Giả sử đang dùng biểu đồ đầu tiên trong sheet
    Set cht = ActiveSheet.ChartObjects(1)

    ' Thêm series mới vào chart
    Set newSeries = cht.Chart.SeriesCollection.NewSeries

    ' Gán dữ liệu cho series mới (ví dụ: cột A là x, cột B là y)
    With newSeries
        .XValues = ActiveSheet.Range("A2:A10")
        .Values = ActiveSheet.Range("B2:B10")
        .Name = "Series mới"
    End With

    MsgBox "Đã thêm series mới vào biểu đồ!"
End Sub
